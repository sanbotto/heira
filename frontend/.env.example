# WalletConnect (required for RainbowKit, WalletConnect, and Ledger connectors)
VITE_WALLETCONNECT_PROJECT_ID=

# Coinbase CDP API
VITE_COINBASE_PROJECT_ID=
VITE_COINBASE_API_KEY_ID=
VITE_COINBASE_API_KEY_SECRET=

# Chainlink
VITE_CHAINLINK_ORG_ID=

# World ID
VITE_WORLD_ID_API_KEY=
VITE_WORLD_ID_APP_ID=

# 1inch API (for token prices)
VITE_ONEINCH_API_KEY=

# Contract Addresses (will be set after deployment)
VITE_FACTORY_ADDRESS_ETHEREUM=
VITE_FACTORY_ADDRESS_BASE=
VITE_FACTORY_ADDRESS_CITREA=

# Backend API URL (for API endpoints like verify-escrow)
VITE_BACKEND_API_URL=http://localhost:3001

# Private key for Filecoin deployment (NEVER commit this)
PRIVATE_KEY=your_private_key_here

# Storacha Network space DID (for Filecoin deployment)
# Get your space DID from: https://console.storacha.network/
STORACHA_SPACE=did:key:XYZ
